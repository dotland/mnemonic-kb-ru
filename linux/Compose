# UTF-8 (Unicode) compose sequences
#
# Modified for Russian (mnemonic) by [Dotland](https://github.com/dotland)
#
# Make sure GTK_IM_MODULE="xim" is set to support multichar input.
#
# Use the sequences from en_US.UTF-8 as the basis:
# include "/usr/share/X11/locale/en_US.UTF-8/Compose"

# Symbols accessed by dead_hook
<dead_hook> <space>                     : "/"   slash # SOLIDUS
<dead_hook> <dead_hook>                 : "//"
<dead_hook> <equal>                     : "≠"   notequal # NOT EQUAL TO
<dead_hook> <Cyrillic_el>               : "∠"   U2220 # ANGLE
<dead_hook> <Cyrillic_EL>               : "∠"   U2220 # ANGLE
<dead_hook> <asterisk>                  : "⋆"   U22C6 # STAR OPERATOR
<dead_hook> <Cyrillic_ie>               : "∄"   U2204 # THERE DOES NOT EXIST
<dead_hook> <Cyrillic_IE>               : "∄"   U2204 # THERE DOES NOT EXIST
<dead_hook> <Cyrillic_ve>               : "∧"   upcaret # LOGICAL AND
<dead_hook> <Cyrillic_VE>               : "∧"   upcaret # LOGICAL AND
<dead_hook> <Cyrillic_en>               : "¬"   notsign # NOT SIGN
<dead_hook> <Cyrillic_EN>               : "¬"   notsign # NOT SIGN
<dead_hook> <0>                         : "∅"   U2205 # EMPTY SET
<dead_hook> <Cyrillic_be>               : "∉"   U2209 # NOT AN ELEMENT OF
<dead_hook> <Cyrillic_BE>               : "∉"   U2209 # NOT AN ELEMENT OF
<dead_hook> <Cyrillic_u>                : "∩"   upshoe # INTERSECTION
<dead_hook> <Cyrillic_U>                : "∩"   upshoe # INTERSECTION
<dead_hook> <dead_caron>                : "⊄"   U2284 # NOT A SUBSET OF
<dead_hook> <dead_cedilla>              : "⊄"   U2284 # NOT A SUBSET OF
<dead_hook> <greater>                   : "▷"   U21A6 # WHITE RIGHT-POINTING TRIANGLE
<dead_hook> <less>                      : "◁"   U21B5 # WHITE LEFT-POINTING TRIANGLE
<dead_hook> <Cyrillic_softsign>         : "′"   minutes # PRIME
<dead_hook> <Cyrillic_SOFTSIGN>         : "″"   seconds # DOUBLE PRIME
<dead_hook> <semicolon>                 : "‴"   U2034 # TRIPLE PRIME

